	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Graphics">
	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.Graphics.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Input">
	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.Input.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Platforms">
	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.Platforms.dll</HintPath>
    </Reference>

    </Reference>
    <Reference Include="DeltaEngine.Rendering2D.Particles">
	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.Rendering2D.Particles.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Scenes">
	  <HintPath>$(DeltaEnginePath)\MonoGame\DeltaEngine.Scenes.dll</HintPath>

<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProjectGuid>{$guid2$}</ProjectGuid>
    <OutputType>WinExe</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>$safeprojectname$</RootNamespace>
    <AssemblyName>$safeprojectname$</AssemblyName>
    <TargetFrameworkVersion>v4.0</TargetFrameworkVersion>
    <FileAlignment>512</FileAlignment>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug</OutputPath>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release</OutputPath>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>SideScrollerIcon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="DeltaEngine">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Graphics">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Graphics.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Input">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Input.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Physics2D">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Physics2D.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Shapes">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Shapes.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Sprites">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Sprites.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Scenes">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Scenes.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.WindowsMonoGame">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.WindowsMonoGame.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Content Include="Sidescroller.ico" />
  </ItemGroup>
  <ItemGroup>
    <None Include="app.config" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="EnemyPlane.cs" />
    <Compile Include="GameControls.cs" />
    <Compile Include="InteractionLogics.cs" />
    <Compile Include="Menu.cs" />
    <Compile Include="ParallaxBackground.cs" />
    <Compile Include="Plane.cs" />
    <Compile Include="PlayerPlane.cs" />
    <Compile Include="DefRenderLayer.cs" />
    <Compile Include="Game.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="Velocity2D.cs" />
  </ItemGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
</Project>
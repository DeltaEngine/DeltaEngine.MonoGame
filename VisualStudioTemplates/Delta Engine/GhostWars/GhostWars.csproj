<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProjectGuid>{$guid2$}</ProjectGuid>
    <OutputType>WinExe</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>$safeprojectname$</RootNamespace>
    <AssemblyName>$safeprojectname$</AssemblyName>
    <TargetFrameworkVersion>v4.0</TargetFrameworkVersion>
    <FileAlignment>512</FileAlignment>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug</OutputPath>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release</OutputPath>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <PlatformTarget>x86</PlatformTarget>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>GhostWarsIcon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="DeltaEngine.Content.Xml">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Content.Xml.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Input">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Input.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Fonts">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Fonts.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Particles">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Particles.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Shapes">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Shapes.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.Rendering.Sprites">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.Rendering.Sprites.dll</HintPath>
    </Reference>
    <Reference Include="DeltaEngine.WindowsMonoGame">
	  <HintPath>$(DeltaEnginePath)\DeltaEngine.WindowsMonoGame.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Content Include="GhostWars.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Properties\AssemblyInfo.cs" />
    <Compile Include="Effects.cs" />
    <Compile Include="GameLogic.cs" />
    <Compile Include="GameState.cs" />
    <Compile Include="MainMenu.cs" />
    <Compile Include="Game.cs" />
    <Compile Include="Team.cs" />
    <Compile Include="Tree.cs" />
    <Compile Include="TreeManager.cs" />
    <Compile Include="GhostWave.cs" />
  </ItemGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
</Project>